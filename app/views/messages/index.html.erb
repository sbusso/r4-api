<script>
App.messages = App.cable.subscriptions.create('MessagesChannel', {  
  received: function(data) {
    console.log(data);
    return $('#messages').append(this.renderMessage(data));
  },

  renderMessage: function(data) {
    return "<h2>" + data.message.new_val.content + "</h2>";
  }
});
</script>

<h1>Messages:</h1>
<div id='messages'>
  <h2>First Line</h2>
</div>
